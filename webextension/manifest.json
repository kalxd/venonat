{
	"manifest_version": 2,
	"name": "venonat",
	"author": "荀涧林",
	"description": "一个docker register的http接口可视化图形界面，方便日常镜像管理。",
	"version": "0.2.3",
	"icons": {
		"32": "assert/32.png",
		"64": "assert/64.png"
	},

	"options_ui": {
		"page": "assert/option.html"
	},
	"browser_action": {
		"default_title": "docker register GUI",
		"default_icon": {
			"16": "assert/16.png",
			"32": "assert/32.png"
		}
	},

	"background": {
		"scripts": ["dist/background.js"]
	},

	"permissions": [
		"storage",
		"clipboardWrite",
		"<all_urls>"
	]
}
